{
  "name": "@redhat-cloud-services/policies-client",
  "version": "3.0.4",
  "description": "",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/esm/api.js",
      "default": "./dist/api.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/esm/*/index.js",
      "default": "./dist/*/index.js"
    }
  },
  "scripts": {
    "doc": "typedoc"
  },
  "typesVersions": {
    "*": {
      "api": [
        "./dist/api.d.ts"
      ],
      "*": [
        "./dist/*/index.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RedHatInsights/javascript-clients.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/RedHatInsights/javascript-clients/issues"
  },
  "homepage": "https://github.com/RedHatInsights/javascript-clients#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tslib": "^2.6.2",
    "@redhat-cloud-services/javascript-clients-shared": "^2.0.3"
  }
}
